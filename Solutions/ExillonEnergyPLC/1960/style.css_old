input[type=button] {
}

input[type=text], input[type=email] {
    font-size: 14px;
    line-height: 30px;
    height: 30px;
}

input[type=checkbox] {
    font-size: 10px;
}

p {
    font-size: 16px;
}

#searchtext {
    font-size: 14px;
    width: 32%;
