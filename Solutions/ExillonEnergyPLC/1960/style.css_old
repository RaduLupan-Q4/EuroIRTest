}

h2 {
}

.textArea {
    color: #337629;
    font-size: 14px;
    display: block;
    float: left;
    width: 10%;
    min-width: 150px;
}

.form_field {
    vertical-align: top;
    font-size: 12px;
    margin: 0px;
}

.buttonStyle {
background-color: #858585;
border: none;
padding: 5px 20px;
-webkit-border-radius: 20px;
-moz-border-radius: 20px;
-webkit-appearance: none;
border-radius: 20px;
font-size: 14px;
color: #fff;
cursor: pointer;
}

    .buttonStyle:hover {
    }

   

.radio {
    background-image: url(images/radio.png);
    background-repeat: no-repeat;
    background-position: left;
    cursor: pointer;
    padding-left: 20px;
    height: 40px;
    line-height: 40px;
    float: left;
}

    .radio.checked {
        background-image: url(images/radiocheck.png);
        background-repeat: no-repeat;
        cursor: pointer;
    }

.inputTextTitle {
font-weight:normal;
    min-width: 102px;
}

.Figure {
    font-size: 16px;
}

.divideLine {
    margin-top: 5px;
    margin-bottom: 10px;
    border-bottom: 1px solid #c5b6b8;
}

/* Tables */
.IRTable {
    width: 100%;
    border-collapse: collapse;
    border-bottom: 1px solid #C5B6B8 !important;
}

    .IRTable th, .IRTable td ,.headline{
        font-size: 16px;
padding:10px 0;
        text-align: left;
        color: #333333;
    }

    .IRTable th, .headline {
    font-weight:normal;
       background: #858585;
        color: #fff;

    }

    .IRTable td {
        background-color: #fff;
        border-bottom: 1px solid #e4dfe0;
    }

        .IRTable td.alt {
            background-color: #fafafa;
        }

.tableHighlight {
}

td.newsRowHover {
}

td.filterRow {
}

.IRTable td.rnsTitle {
    color: #19398a;
}

.newsRowCell {
    cursor: pointer;
}
/*
    Alt color : #D1DFF1    
*/
.hide {
    display: none;
}
/* Lookup results table */
#quotetable {
    width: 100%;
    border-collapse: collapse;
    border-spacing: 2px;
}

    #quotetable th, #quotetable td {
        text-align: left;
        padding: 15px;
        padding-left: 3px;
        padding-right: 3px;
        height: 10px;
    }

    #quotetable th {
        background-color: #D6CFC9;
        background-image: url('Images/th_back.png');
        background-repeat: repeat-x;
    font-weight:normal;
        color: #5F564D;
        line-height: 10px;
    }

    #quotetable td {
        line-height: 10px;
        border-collapse: collapse;
        border-bottom: 1px solid #D7CEC9;
        line-height: 10px;
        padding-top: 8px;
        padding-bottom: 6px;
    }

        #quotetable td.alt {
            background-color: #F0EFED;
        }

    #quotetable th.sort {
    }

.IRTableQuote {
    border-spacing: 0px;
}

    .IRTableQuote th, .IRTableQuote td {
        padding: 3px 7px;
    }

    .IRTableQuote th {
        text-align: left;
    font-weight:normal;
    }

    .IRTableQuote td {
        text-align: right;
    }

.IRTableMiniQuote {
    border-collapse: collapse;
}

    .IRTableMiniQuote th, .IRTableMiniQuote td {
        line-height: 24px;
        font-size: 13px;
        border-bottom: 1px solid #000;
    }

    .IRTableMiniQuote th {
        text-align: left;
        padding-left: 2px;
    }

    .IRTableMiniQuote td {
        text-align: right;
    }

.lookupCalc {
    width: 100%;
}

    .lookupCalc th, .lookupCalc td {
    font-weight:normal;
    }

    .lookupCalc th {
        text-align: left;
    }

    .lookupCalc td {
        text-align: right;
        line-height: 24px;
    }

.imgouter {
}

    .imgouter img {
        position: relative;
        padding-top: 0px;
    }

/* Spacers */
.horiSpacer20 {
    height: 20px;
    background: transparent;
    width: 100%;
    clear: left;
    clear: right;
}

.horiSpacer40 {
    height: 40px;
    background: transparent;
    width: 100%;
    clear: left;
    clear: right;
}

/* Links */
a {
    text-decoration: underline;
}

    a:link, a:visited {
        color: #666666;
    }

    a:hover, a:active {
        text-decoration: none;
        color: #CC0000;
    }

    a:link.printerFriendly {
        padding-left: 20px;
        background: url(images/printer_TransBG_simple2_Out.gif) no-repeat;
    }

    a:visited.printerFriendly {
        padding-left: 20px;
        background: url(images/printer_TransBG_simple2_Out.gif) no-repeat;
    }

    a:hover.printerFriendly {
        padding-left: 20px;
        background: url(images/printer_TransBG_simple2_Hover.gif) no-repeat;
    }

    a:active.printerFriendly {
        padding-left: 20px;
        background: url(images/printer_TransBG_simple2_Hover.gif) no-repeat;
    }

.disclaimer {
